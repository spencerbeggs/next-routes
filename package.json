{
  "name": "@spencerbeggs/next-routes",
  "version": "3.1.0",
  "description": "Easy to use universal dynamic routes for Next.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spencerbeggs/next-routes.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist",
  "typings": "typings/next-routes.d.ts",
  "files": [
    "dist",
    "typings"
  ],
  "scripts": {
    "lint": "standard 'src/*' 'test/*'",
    "build": "del dist/* && babel src -d dist",
    "prepublishOnly": "npm run test",
    "pretest": "npm run lint && npm run build",
    "test": "npm run testOnly && npm run testTypings",
    "testOnly": "jest",
    "testTypings": "tsc --project ./typings/tests",
    "dev": "concurrently -k 'npm run build -- -w' 'npm run testOnly -- --watch'",
    "upgrade-interactive": "npm-check --update"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "path-to-regexp": "^3.0.0"
  },
  "peerDependencies": {
    "next": ">=8",
    "react": "^15.4.2 || ^16"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@types/jest": "^24.0.11",
    "@types/next": "^8.0.3",
    "@types/node": "^11.12.0",
    "@zeit/next-typescript": "1.1.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "^24.5.0",
    "concurrently": "4.1.0",
    "del-cli": "1.1.0",
    "jest": "^24.5.0",
    "next": "^8.0.3",
    "npm-check": "^5.9.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-test-renderer": "^16.8.5",
    "react-testing-library": "^6.0.3",
    "standard": "12.0.1",
    "typescript": "^3.3.4000"
  },
  "author": "fridays",
  "license": "MIT",
  "keywords": [
    "next",
    "next.js",
    "react",
    "route",
    "router",
    "routes",
    "routing"
  ]
}
